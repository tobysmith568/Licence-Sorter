  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/node": "^13.11.0",
    "trash": "^6.1.1",
    "tsc": "^1.20150623.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "arg": "^4.1.3",
    "esm": "^3.2.25",
